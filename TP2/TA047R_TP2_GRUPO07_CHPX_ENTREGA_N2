= etiquetas\n","    self.tokenizer = tokenizer\n","    self.max_len = max_len\n","\n","  def __len__(self):\n","    return len(self.resenias)\n","\n","  def __getitem__(self, item):\n","    resenia = str(self.resenias[item])\n","    etiqueta = self.etiquetas[item]\n","    encoding = tokenizer.encode_plus(\n","        resenia,\n","        max_length = self.max_len,\n","        truncation = True,\n","        add_special_tokens = True,\n","        return_token_type_ids = False,\n","        padding = 'max_length',\n","        return_attention_mask = True,\n","        return_tensors = 'pt'\n","    )\n","\n","    return {\n","      'review': resenia,\n","      'input_ids': encoding['input_ids'].flatten(),\n","      'attention_mask': encoding['attention_mask'].flatten(),\n","      'label': torch.tensor(etiqueta, dtype = torch.long)\n","  }"]},{"cell_type":"code","execution_count":15,"metadata":{"id":"6NM4ESivwEqu","executionInfo":{"status":"ok","timestamp":1764717760391,"user_tz":180,"elapsed":1,"user":{"displayName":"Rafael Patty","userId":"17901259879686968883"}}},"outputs":[],"source":["def data_loader(dataset, tokenizer, max_len, batch_size):\n","  dataset = ReseniasDataset(\n","      resenias = dataset.review_es.to_numpy(),\n","      etiquetas = dataset.etiqueta.to_numpy(),\n","      tokenizer = tokenizer,\n","      max_len = MAX_LEN\n","  )\n","\n","  return DataLoader(dataset, batch_size = BATCH_SIZE, num_workers = 4)"]},{"cell_type":"code","execution_count":16,"metadata":{"id":"OQqGNP7Dy2gC","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1764717760408,"user_tz":180,"elapsed":15,"user":{"displayName":"Rafael Patty","userId":"17901259879686968883"}},"outputId":"4ee14897-298c-4b42-c3ae-921105f97315"},"outputs":[{"output_type":"stream","name":"stderr","text":["/usr/local/lib/python3.12/dist-packages/torch/utils/data/dataloader.py:627: UserWarning: This DataLoader will create 4 worker processes in total. Our suggested max numbe