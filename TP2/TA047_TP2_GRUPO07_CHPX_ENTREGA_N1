ng_dim"],"metadata":{"id":"JGEAKNhHNxIt","executionInfo":{"status":"ok","timestamp":1764724751047,"user_tz":180,"elapsed":2,"user":{"displayName":"Rafael Patty","userId":"17901259879686968883"}}},"execution_count":35,"outputs":[]},{"cell_type":"code","source":["class BetterNN(nn.Module):\n","    def __init__(self, weights_matrix, hidden_size=128, num_layers=2, dropout=0.3):\n","        super().__init__()\n","\n","        self.embedding, num_embeddings, embedding_dim = create_emb_layer(weights_matrix, non_trainable=False)\n","\n","        self.lstm = nn.LSTM(\n","            input_size=embedding_dim, # Tamañp de los vectores de palabras.\n","            hidden_size=hidden_size, # \"Memoria\" que tiene cada neurona.\n","            num_layers=num_layers,\n","            batch_first=True,\n","            bidirectional=True, # Importante: Bidireccional.\n","            dropout=dropout if num_layers > 1 else 0\n","        )\n","\n","        self.dropout = nn.Dropout(dropout)\n","\n","        self.fc1 = nn.Linear(hidden_size * 2, 64) # Creamos una capa de 64 neuronas.\n","        self.relu = nn.ReLU()\n","        self.fc2 = nn.Linear(64, 1) # Creamos una capa de 1 neurona para una salida binaria.\n","\n","    def forward(self, inp):\n","\n","        emb = self.embedding(inp)\n","        output, (hidden, cell) = self.lstm(emb)\n","        hidden_final = torch.cat((hidden[-2,:,:], hidden[-1,:,:]), dim=1)\n","\n","        x = self.dropout(hidden_final)\n","        x = self.fc1(x)\n","        x = self.relu(x)\n","        x = self.dropout(x)\n","        out = self.fc2(x)\n","\n","        return out.squeeze(1)"],"metadata":{"id":"SnViKzX0fRJm","executionInfo":{"status":"ok","timestamp":1764724751846,"user_tz":180,"elapsed":22,"user":{"displayName":"Rafael Patty","userId":"17901259879686968883"}}},"execution_count":36,"outputs":[]},{"cell_type":"code","source":["def text_pipeline(text):\n","    tokens = preprocesar_texto_embedding(text).split()\n","    indices = [vocabulario[token] if token in vocabulario else 0 for token in tokens]\n","    return torch.tensor(indices, dtype=torch.long)"],"metadata":{"id":"5XPlZm6J1qse","executionInfo":{"status":"ok","timestamp":1764724753772,"user_tz":180,"elapsed":12,"user":{"displayName":"Rafael Patty","userId":"17901259879686968883"}}},"execution_count":37,"outputs":[]},{"cell_type":"code","source":["class ReseniasDataset(Dataset):\n","    def __init__(self, X, y):\n","        self.X = X.reset_index(drop=True)\n","        self.y = y.reset_index(drop=True).astype(float)\n","\n","    def __len__(self):\n","        return len(self.X)\n","\n","    def __getitem__(self, idx):\n","        return text_pipeline(self.X[idx]), torch.tensor(self.y[idx], dtype=torch.float32)"],"metadata":{"id":"O0l26opW1tDA","executionInfo":{"status":"ok","timestamp":1764724754472,"user_tz":180,"elapsed":4,"user":{"displayName":"Rafael Patty","userId":"17901259879686968883"}}},"execution_count":38,"outputs":[]},{"cell_type":"code","source":["def collate_batch(batch):\n","    texts, labels = zip(*batch)\n","    lengths = [len(t) for t in texts]\n","    max_len = max(lengths)\n","\n","    padded = []\n","    for seq in texts:\n","        pad_size = max_len - len(seq)\n","        padded.append(torch.cat([seq, torch.zeros(pad_size, dtype=torch.long)]))\n","\n","    return torch.stack(padded), torch.tensor(labels, dtype=torch.float32)"],"metadata":{"id":"kxyAXlcp1uli","executionInfo":{"status":"ok","timestamp":1764724755910,"user_tz":180,"elapsed":15,"user":{"displayName":"Rafael Patty","userId":"17901259879686968883"}}},"execution_count":39,"outputs":[]},{"cell_type":"code","source":["BATCH_SIZE = 64\n","\n","train_dataset = ReseniasDataset(x_train_tex, y_train_tex)\n","test_dataset = ReseniasDataset(x_test_tex, y_test_tex)\n","\n","train_loader = DataLoader(train_dataset, batch_size=BATCH_SIZE,\n","                          shuffle=True, collate_fn=collate_batch)\n","test_loader = DataLoader(test_dataset, batch_size=BATCH_SIZE,\n","                         shuffle=False, collate_fn=collate_batch)"],"metadata":{"id":"_MbsoxCd1wTc","executionInfo":{"status":"ok","timestamp":1764724764751,"user_tz":180,"elapsed":42,"user":{"displayName":"Rafael Patty","userId":"17901259879686968883"}}},"execution_count":40,"outputs":[]},{"cell_type":"code","source":["device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n","\n","model = BetterNN(weights_matrix, hidden_size=128, num_layers=2, dropout=0.5).to(device)\n","\n","criterion = nn.BCEWithLogitsLoss()\n","\n","# A veces un LR más bajo ayuda cuando descongelamos embeddings\n","optimizer = optim.Adam(model.parameters(), lr=0.0005, weight_decay=1e-5)\n","\n","epochs = 3 # Aumentar un poco las épocas\n","\n","for epoch in range(epochs):\n","    model.train()\n","    total_loss = 0\n","    correct = 0\n","    total = 0\n","\n","    for inputs, labels in train_loader:\n","        inputs, labels = inputs.to(device), labels.to(device)\n","\n","        optimizer.zero_grad()\n","        logits = model(inputs)\n","        loss = criterion(logits, labels)\n","\n","        loss.backward()\n","\n","        nn.utils.clip_grad_norm_(model.parameters(), max_norm=5)\n","\n","        optimizer.step()\n","        total_loss += loss.item()\n","\n","        # Calcular accuracy simple para ver si aprende\n","        probs = torch.sigmoid(logits)\n","        preds = (probs > 0.5).float()\n","        correct += (preds == labels).sum().item()\n","        total += labels.size(0)\n","\n","    avg_loss = total_loss / len(train_loader)\n","    acc = correct / total\n","    print(f\"Época {epoch+1}/{epochs} - Loss: {avg_loss:.4f} - Acc Train: {acc:.4f}\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"BbAlS7WV1zYW","executionInfo":{"status":"ok","timestamp":1764725204766,"user_tz":180,"elapsed":432396,"user":{"displayName":"Rafael Patty","userId":"17901259879686968883"}},"outputId":"9d12c029-bec1-4736-f7d3-b99d0eaaf990"},"execution_count":41,"outpu